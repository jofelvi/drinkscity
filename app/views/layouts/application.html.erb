<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		
		<title>Drinkscity</title>

		<%= favicon_link_tag 'favicon/apple-touch-icon-57x57.png' %>
		<%= csrf_meta_tags %>

		<script src="https://maps.google.com/maps/api/js?key=AIzaSyB2CMwToy9v3u2GvEAkNfCEP6URjOgxK1w&libraries=places" type="text/javascript"></script>
		<script src="https://www.gstatic.com/charts/loader.js"></script>
		<script src="https://webrtc.github.io/adapter/adapter-latest.js"></script>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'admin', 'data-turbolinks-track': 'reload' %>
	</head>

	<body class="nav-md">
		<div class="container body">
			<div class="main_container">
				<div class="col-md-3 left_col">
					<div class="left_col scroll-view">
						<%= render partial: 'layouts/shared/leftnav' %>
					</div>
				</div>
				<div class="top_nav">
					<div class="nav_menu" style="background-image: url(/assets/banda.png); color: white">
						<nav>
							<%= render partial: 'layouts/shared/topnav'%>
						</nav>
					</div>
				</div>
				<div class="right_col" role="main">
					<%= yield %>
				</div>
				<footer>
					<!--	<%= render partial: 'layouts/shared/footer' %> -->
				</footer>
			</div>
		</div>
		<%= render partial: 'layouts/shared/modal' %>

	</body>
</html>
